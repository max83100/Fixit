<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".parts.mig_models.MIG_200Energy_parts">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:orientation="vertical">

    <Button
        android:id="@+id/mig200en_inside"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:background="@drawable/button_shape"
        android:text=" Фото внутренностей" />

    <TextView
        android:id="@+id/mig200en_europlug"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textIsSelectable="true"
        android:layout_weight="1"
        android:gravity="center_horizontal"
        android:text="Евроразъём "
        android:textColor="#151515"
        android:textSize="24sp" />

    <com.github.chrisbanes.photoview.PhotoView
        android:id="@+id/mig200en_europlug_image"
        android:layout_width="match_parent"
        android:layout_height="350dp"
        android:layout_marginBottom="20dp"
        android:layout_weight="1"
        app:srcCompat="@drawable/ic_launcher_foreground" />

    <TextView
        android:id="@+id/mig200en_polarity_pin"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textIsSelectable="true"
        android:layout_weight="1"
        android:gravity="center_horizontal"
        android:text="Штырь смены полярности"
        android:textColor="#151515"
        android:textSize="24sp" />

    <com.github.chrisbanes.photoview.PhotoView
        android:id="@+id/mig200en_polarity_pin_image"
        android:layout_width="match_parent"
        android:layout_height="350dp"
        android:layout_marginBottom="20dp"
        android:layout_weight="1"
        app:srcCompat="@drawable/ic_launcher_foreground" />

    <TextView
        android:id="@+id/mig200en_invertor_board"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textIsSelectable="true"
        android:layout_weight="1"
        android:gravity="center_horizontal"
        android:text="Плата инвертора \n артикул DX50009"
        android:textColor="#151515"
        android:textSize="24sp" />

    <com.github.chrisbanes.photoview.PhotoView
        android:id="@+id/mig200en_invertor_board_image"
        android:layout_width="match_parent"
        android:layout_height="350dp"
        android:layout_marginBottom="20dp"
        android:layout_weight="1"
        app:srcCompat="@drawable/ic_launcher_foreground"
        tools:ignore="VectorDrawableCompat" />

    <TextView
        android:id="@+id/mig200en_front_board"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textIsSelectable="true"
        android:layout_weight="1"
        android:gravity="center_horizontal"
        android:text="Плата индикации \n артикул DX01577"
        android:textColor="#151515"
        android:textSize="24sp" />

    <com.github.chrisbanes.photoview.PhotoView
        android:id="@+id/mig200en_front_board_image"
        android:layout_width="match_parent"
        android:layout_height="350dp"
        android:layout_marginBottom="20dp"
        android:layout_weight="1"
        app:srcCompat="@drawable/ic_launcher_foreground" />

    <TextView
        android:id="@+id/mig200en_plastic"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textIsSelectable="true"
        android:layout_weight="1"
        android:gravity="center_horizontal"
        android:text="Комплект пластиковых панелей"
        android:textColor="#151515"
        android:textSize="24sp" />

    <com.github.chrisbanes.photoview.PhotoView
        android:id="@+id/mig200en_plastic_image"
        android:layout_width="match_parent"
        android:layout_height="350dp"
        android:layout_marginBottom="20dp"
        android:layout_weight="1"
        app:srcCompat="@drawable/ic_launcher_foreground" />

    <TextView
        android:id="@+id/mig200en_control_board"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textIsSelectable="true"
        android:layout_weight="1"
        android:gravity="center_horizontal"
        android:text="Плата управления \n артикул DX01577"
        android:textColor="#151515"
        android:textSize="24sp" />

    <com.github.chrisbanes.photoview.PhotoView
        android:id="@+id/mig200en_control_board_image"
        android:layout_width="match_parent"
        android:layout_height="350dp"
        android:layout_marginBottom="20dp"
        android:layout_weight="1"
        app:srcCompat="@drawable/ic_launcher_foreground" />

    <TextView
        android:id="@+id/mig200en_mech"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:gravity="center_horizontal"
        android:textIsSelectable="true"
        android:text="Механизм подачи"
        android:textColor="#151515"
        android:textSize="24sp" />

    <com.github.chrisbanes.photoview.PhotoView
        android:id="@+id/mig200en_mech_image"
        android:layout_width="match_parent"
        android:layout_height="350dp"
        android:layout_marginBottom="20dp"
        android:layout_weight="1"
        app:srcCompat="@drawable/ic_launcher_foreground" />

    <TextView
        android:id="@+id/mig200en_fan"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:gravity="center_horizontal"
        android:textIsSelectable="true"
        android:text="Вентилятор"
        android:textColor="#151515"
        android:textSize="24sp" />

    <com.github.chrisbanes.photoview.PhotoView
        android:id="@+id/mig200en_fan_image"
        android:layout_width="match_parent"
        android:layout_height="350dp"
        android:layout_marginBottom="20dp"
        android:layout_weight="1"
        app:srcCompat="@drawable/ic_launcher_foreground" />
</LinearLayout>
</ScrollView>